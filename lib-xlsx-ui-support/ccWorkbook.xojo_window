#tag WindowCode
	#tag Method, Flags = &h0
		Sub UseWorkbook(workbook as clWorkbook)
		  self.MyWorkbook = workbook
		  
		  lbl_Filename.Text = workbook.SourceFile.Name
		  
		  SheetsToPopup(self.MyWorkbook, PopupMenu1)
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		MyWorkbook As clWorkbook
	#tag EndProperty


#tag EndWindowCode

#tag Events PopupMenu1
	#tag Event
		Sub SelectionChanged(item As DesktopMenuItem)
		  
		  SheetToListBox( self.MyWorkbook, me.SelectedRowText, Listbox1)
		   
		   
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ListBox1
	#tag Event
		Function CellPressed(row As Integer, column As Integer, x As Integer, y As Integer) As Boolean
		  
		  var tmp as string = me.CellTextAt(row, column)
		  
		  lbl_zoom.Text = tmp
		  
		  
		End Function
	#tag EndEvent
#tag EndEvents
